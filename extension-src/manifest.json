{
     "manifest_version": 3,
     "name": "ProcrastinationBuster",
     "version": "1.0.0",
     "description": "Control your attention span and boost productivity",
     "permissions": [
          "storage",
          "tabs",
          "notifications",
          "alarms",
          "declarativeNetRequest"
     ],
     "host_permissions": [
          "<all_urls>"
     ],
     "declarative_net_request": {
          "rule_resources": [
               {
                    "id": "ruleset_1",
                    "enabled": true,
                    "path": "rules.json"
               }
          ]
     }
}