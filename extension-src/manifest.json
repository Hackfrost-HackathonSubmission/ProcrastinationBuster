{
     "manifest_version": 3,
     "name": "ProcrastinationBuster",
     "version": "1.0.0",
     "description": "Control your attention span and boost productivity",
     "permissions": [
          "storage",
          "tabs",
          "notifications",
          "alarms",
          "declarativeNetRequest"
     ],
     "action": {
          "default_popup": "popup.html"
     },
     "background": {
          "service_worker": "background.js",
          "type": "module"
     },
     "host_permissions": [
          "<all_urls>"
     ]
}