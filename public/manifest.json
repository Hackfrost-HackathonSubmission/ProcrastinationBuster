{
  "manifest_version": 3,
  "name": "ProcrastinationBuster",
  "version": "1.0",
  "description": "A browser extension to help control your attention span",
  "permissions": [
    "storage",
    "webNavigation",
    "webRequest"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "out/index.html"
  }
}